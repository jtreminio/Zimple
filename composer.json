{
    "name": "jtreminio/zimple",
    "type": "library",
    "description": "A wrapper for Pimple container that adds several useful features to make it easier to manage your code!",
    "keywords": ["pimple", "container", "dependency injection", "inversion of control", "dic", "ioc", "testing", "unit testing"],
    "homepage": "https://github.com/jtreminio/Zimple",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Treminio",
            "email": "jtreminio@gmail.com",
            "homepage": "http://jtreminio.com"
        }
    ],
    "require": {
        "php":           ">=5.3.0",
        "pimple/pimple": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "jtreminio\\Zimple": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
